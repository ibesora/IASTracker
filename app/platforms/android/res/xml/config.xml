<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="com.ic5team.iastracker" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Battery">
        <param name="android-package" value="org.apache.cordova.batterystatus.BatteryListener" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="Globalization">
        <param name="android-package" value="org.apache.cordova.globalization.Globalization" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <splash density="port-hdpi" src="res/splash.png" />
    <splash density="port-ldpi" src="res/splash.png" />
    <splash density="port-mdpi" src="res/splash.png" />
    <splash density="port-xhdpi" src="res/splash.png" />
    <allow-intent href="market:*" />
    <preference name="KeepRunning" value="false" />
    <name>IASTracker</name>
    <description>
        The IASTracker ic5Team project
    </description>
    <icon density="mdpi" height="57" platform="ios" src="res/IASTRACKER-map_V_Ok.png" width="57" />
    <author email="dev@altersport.cat" href="http://www.altersport.cat">
        Alter Sport
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="portrait" />
    <preference name="android-minSdkVersion" value="19" />
</widget>
